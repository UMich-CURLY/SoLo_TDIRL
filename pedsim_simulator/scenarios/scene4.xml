    <agent x="30" y="7" n="15" dx="10" dy="6" type="1">
        <addwaypoint id="left_up"/>
        <addwaypoint id="left_down"/>
        <addwaypoint id="right_down"/>
        <addwaypoint id="right_up"/>
    </agent>
    <agent x="10" y="7" n="15" dx="10" dy="6" type="1">
        <addwaypoint id="left_up"/>
        <addwaypoint id="left_down"/>
        <addwaypoint id="right_down"/>
        <addwaypoint id="right_up"/>
    </agent>
